						<div class="f-cb ">
							<a :href="'store_pro.html?id='+item.id+'&company='+item.companyId" target="_blank" class="fl fsize12 co66 hovere42008 pb20 pl20 pr20 pt25 home-pro rel" v-for="(item,index) in list">
