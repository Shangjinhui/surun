								<div class="abs center pro-eva-tx fsize12 co66">
									<img :src="item.user.avatar?item.user.avatar:'static/img/banner1.png'" class="img-cover mar-cen mb5">
									<div class="">{{item.user.nickname}}</div>
								</div>
								<div class="bor-bof2f4f6 pb20">
