		img{
			width: 76px;
			height:76px;
		}

		&:after{
			position: absolute;
			content: '';
			right:0;
			top:50%;
			transform: translateY(-50%);
			height:20px;
			background: #ececec;
			width: 1px;
		}
	}
	.header-top-fl{
		margin-top: 27px;
		padding-bottom: 10px;

		.img1{
			vertical-align: 3px;
		}
		.img2{
			vertical-align: -4px;
		}

		.header-top-fl-box{
			width: 390px;
			top:25px;
			left:0;
			border: 1px solid #ececec;
			padding:20px 20px 10px 20px;
			z-index:3;
			-webkit-transition: opacity 0.3s, z-index 0.3s;
			transition: opacity 0.3s, z-index 0.3s;
			opacity: 0;
			visibility: hidden;


			.img{
				width: 54px;
				height:54px;
			}
			.ico{
				background: #f08519;
				color: #fff;
				border-radius: 50%;
				width: 24px;
				line-height: 24px;
				text-align: center;
				transform: scale(0.8);
			}

			.header-top-btn{
				width: 140px;

				span{
					width: 20px;
					height:20px;
					display: inline-block;
					margin-right:5px;
					vertical-align: -5px;

					&.ico1{background-position: -160px 0;}
					&.ico2{background-position: -186px 0;}
					&.ico3{background-position: -290px -30px;}

				}
			}
		}

		&:hover {
			.header-top-fl-box{
				opacity: 1;
				z-index: 100;
				visibility: visible;
			}

		}
	}
