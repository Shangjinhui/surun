.help-banner{
	width: 100%;
	position: relative;
	height: 147px;
}
.help-banner .box{
	width: 510px;
	height: 38px;
	background-color: #fff;
	border-radius: 30px;
	overflow: hidden;
	position: absolute;
	left: 50%;
	top: 50%;
	transform: translate(-50%,-50%);
}
.help-banner .box input{
	display: block;
	width: 440px;
	height: 30px;
	line-height: 30px;
	box-sizing:border-box;
	padding-left: 27px;
	font-size: 14px;
	color: #999999;
	float: left;
	margin-top: 4px;
}
.help-banner .box .submit{
	width: 63px;
	height: 38px;
	line-height: 38px;
	text-align: center;
	color: #fff;
	background-color: #c12921;
	border-radius: 30px;
	float: right;
	font-size: 16px;
	cursor: pointer;
}


