.nav-list .title i{
	width: 10px;
	height: 6px;
	background: url(../img/l-4.png) center no-repeat;
	transition:all 0.3s linear;
    -webkit-transition:all 0.3s linear;
    display: block;
    position: absolute;
    right: 20px;
    top: 50%;
    margin-top: -3px;
}
.nav-list li .title i.on{
	transform:rotate(180deg);
	-webkit-transform:rotate(180deg);
}
.nav-list .tit i{
	display: block;
	width: 6px;
	height: 10px;
	position: absolute;
	right: 22px;
	top: 50%;
	margin-top: -5px;
	background: url(../img/l-5.png) center no-repeat;
	opacity: 0;
	filter: alpha(opacity=0);
	transition:all 0.3s linear;
    -webkit-transition:all 0.3s linear;
}
.nav-list li .tit.on i,
.nav-list li .tit:hover i{
	opacity: 1;
	filter: alpha(opacity=100);
}
.nav-list li .child{display: none;}
.nav-list .tit:before{
	display: block;
	width: 4px;
	height: 2px;
	background-color: #bbbbbb;
	position: absolute;
	left: 30px;
	top: 50%;
	margin-top:-1px;
	content: '';
}
.nav-list .tit:hover{
	color: #c4423c;
	background-color: #fdf7f6;
}
.nav-list .tit:hover:before{
	background-color: #c4423c;
}
