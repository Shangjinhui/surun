.nav-hover:before{
	content: '';
	width: 4px;
	height: 0px;
	background-color: #e42008;
	position: absolute;
	left: 1px;
	top: 50%;
	transform: translateY(-50%);
	display: block;
	transition: all 0.29s;
	-webkit-transition: all 0.29s;
}
.nav-hover:after{
	content: '';
	width: 0px;
	height: 10px;
	position: absolute;
	right: 22px;
	top: 50%;
	transform: translateY(-50%);
	transition: all 0.25s;
	-webkit-transition: all 0.25s;
	display: block;
	background: url(../img/center-img2.png) center no-repeat;
}
.nav-hover.on:before,
.nav-hover:hover:before{
	height: 14px;
}
.nav-hover.on:after,
.nav-hover:hover:after{
	width: 5px;
}
.nav-hover:hover,
.nav-hover.on{
	background-color: #fdf7f6;
	color: #e42008;
}
