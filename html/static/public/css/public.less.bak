@import "color.less";
* {
	margin: 0;
	padding: 0;
	border: 0;
	list-style: none;
	-webkit-transition-timing-function: ease-in-out;
	-moz-transition-timing-function: ease-in-out;
	-ms-transition-timing-function: ease-in-out;
	-o-transition-timing-function: ease-in-out;
	transition-timing-function: ease-in-out;
	background-repeat: no-repeat;
	//font-family: "微软雅黑";
	font-family: "微软雅黑", "Source Han Sans CN", "Microsoft YaHei", "Microsoft JhengHei", "Hiragino Sans GB", "PingFang SC", "Helvetica Neue", Helvetica, "Heiti SC", Arial, sans-serif;
	box-sizing: border-box;
}
*:focus {
	outline: none;
}
*, *:before, *:after {
	-webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}
ol, ul, li {
	padding: 0;
	margin: 0;
	display: block;
}
u, i {
	font-size: 14px;
	font-weight: normal;
	font-style: normal;
}
table {
	border-collapse: collapse;
	border-spacing: 0px;
	width: 100%;
}
fieldset, img {
	border: 0;
}
html {
	width: 100%;
}
body {
	width: 100%;
	position: relative;
	font-size: 14px;
	color: #333;
	line-height: 1;
	min-width: 1200px;
}
input, select, textarea {
	-webkit-appearance: none;
	border: none;
	border-radius: 0;
	font-family: "微软雅黑", "Source Han Sans CN", "Microsoft YaHei", "Microsoft JhengHei", "Hiragino Sans GB", "PingFang SC", "Helvetica Neue", Helvetica, "Heiti SC", Arial, sans-serif;
}
a {
	color: #333;
	text-decoration: none;
}
a:link {
	text-decoration: none;
}
a:visited {
	text-decoration: none;
}
.f-x {
	zoom: 1;
}
.f-x:after {
	content: ".";
	display: block;
	height: 0;
	clear: both;
	visibility: hidden
}
