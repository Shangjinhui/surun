.drop-more {
	padding: .32rem 0 .32rem;
}
.drop-more-text {
	display: flex;
	justify-content: center;
	align-items: center;
	height: .3rem;
	line-height: .3rem;
	font-size: .24rem;
	color: #c4c4c4;
}
.drop-more-text i {
	display: block;
	width: .64rem;
	height: 1px;
	background: #c4c4c4;
}
.avatar {
	border-radius: 100%;
	background-size: cover;
}
.mint-msgbox-confirm {
	color: @color;
}
.switch {
	//display: inline-block;
	width: 1.04rem;
	height: 0.6rem;
	border: 1px solid #bbb;
	border-radius: .3rem;
	background: #bbb;
	position: relative;
	&::after {
		content: '';
		top: 0;
		left: 0;
		position: absolute;
		-webkit-transition: -webkit-transform .3s;
		transition: -webkit-transform .3s;
		transition: transform .3s;
		transition: transform .3s, -webkit-transform .3s;
		border-radius: 0.3rem;
		width: .56rem;
		height: .56rem;
		background-color: #fff;
		box-shadow: 0 1px 3px rgba(0, 0, 0, .4);
	}
	&.on {
		background: @color;
		border-color: @color;
		&::after {
			-webkit-transform: translateX(.45rem);
			transform: translateX(.45rem);
		}
	}
}
