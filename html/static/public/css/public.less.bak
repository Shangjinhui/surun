/*新弹窗*/
.back-black {
	width: 100%;
	height: 100%;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 0;
	-webkit-transition: opacity 0.4s, z-index 0.4s;
	transition: opacity 0.4s, z-index 0.4s;
	opacity: 0;
	visibility: hidden;

	.back-black-bg {
		background: rgba(0, 0, 0, 0.6);
		width: 100%;
		height: 100%;
		position: absolute;
		top: 0;
		left: 0;
	}

	&.back-black-show {
		opacity: 1;
		z-index: 100;
		visibility: visible;
	}
}
.back-black-con {
	position: absolute;
	left: 50%;
	top: 50%;
	transform: translate(-50%, -50%);
	background: #ffffff;
	max-height: 80vh;
	box-sizing: border-box;
	border-radius: 4px;
	min-width: 290px;
	overflow: hidden;

	.back-black-tit {
		font-size: 16px;
		background: #f0f0f0;
		line-height: 40px;
		padding: 0px 15px;
	}

	.back-black-btn{
		position: absolute;
		right:15px;
		top:12px;
		width:16px ;
		cursor: pointer;
	}

}
.mint-switch-core {
	width: 1.04rem;
	height: 0.6rem;
	border: 1px solid #bbb;
	background: #bbb;
	border-radius: 0.3rem;

	&::before {
		width: 1rem;
		height: 0.56rem;
		background-color: #bbb;
	}
	&::after {
		width: 0.56rem;
		height: 0.56rem;
		background-color: #fff;
	}
}
.mint-switch-input:checked + .mint-switch-core {
	border-color: @color;
	background-color: @color;
	&::after {
		-webkit-transform: translateX(0.45rem);
		transform: translateX(0.45rem);
	}
}
.mint-datetime-action {
	color: @color;
}
