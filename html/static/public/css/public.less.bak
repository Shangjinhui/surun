//分页
.el-pagination{
	button{
		min-width: 38px;
		font-size: 12px;
		height: 38px;
		border: 1px solid rgba(179, 179, 179, 0.6);
		text-align: center;
		&:hover {
			color: #e42008;
		}
		&.disabled {
			color: #999;
			cursor: not-allowed;
			opacity:.4;
		}
	}
	&.is-background{
		.btn-prev,.btn-next {
			//padding: 0px 15px;
			color: #999;
			background: #ffffff;
		}

	}

	.el-pager {

		li{
			&.number {
				padding: 0 4px;
				border: 1px solid rgba(179, 179, 179, 0.6);
				font-size: 12px;
				min-width: 38px;
				height: 38px;
				line-height: 36px;
				margin: 0px 2px;
				font-weight: normal;
				color: #999;
				background: #ffffff;

				&:hover{
					background: #e42008;
					border-color:#e42008 ;
					color: #fff;
				}
			}
			&.active+li {
				border: 1px solid rgba(179, 179, 179, 0.6);

			}

			&.active {
				color: #fff;
				background: #e42008;
				border-color:#e42008 ;
			}

